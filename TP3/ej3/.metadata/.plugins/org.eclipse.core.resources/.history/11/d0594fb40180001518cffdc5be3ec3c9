public class Main 
{
	public static void main(String[] args) throws Exception 
	{	
		Lector lector = new Lector(args [0]);	
		Grafo grafo = lector.MakeGraph();
		
		DrawMatrizDeAdyacencia(grafo);
		
		//Coloring.MakeRainbow(grafo);
		
		
	}

	private static void DrawMatrizDeAdyacencia(Grafo grafo) {
		boolean[][] matrix = grafo.getMatrizDeAdyacencia();
		int n = matrix.length;
		
		for (int i = 0; i < n; i++) 
		{
		    for (int j = 0; j < n; j++) 
		    {
		        System.out.print(matrix[i][j]? 1 : 0  + " ");
		    }
		    System.out.print("\n");
		}
	}

}
